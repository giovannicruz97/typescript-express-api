{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {
    "prefer-const": true
  },
  "rules": {
    "no-unused-variable": true,
    "no-unused-expression": true,
    "variable-name": false,
    "max-line-length": false,
    "import-name": false
  },
  "rulesDirectory": []
}
